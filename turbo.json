{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "build/**"]
    },
    "lint": { "outputs": [] },
    "format": { "outputs": [] },
    "dev": { "cache": false, "persistent": true },
    "start": { "cache": false, "persistent": true }
  }
}
